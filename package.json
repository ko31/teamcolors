{
  "name": "teamcolors.github.io",
  "version": "1.0.0",
  "description": "Team Colors is a reference of color values for japanese high school baseball teams.",
  "private": true,
  "scripts": {
    "dev": "npm run css:build && npm run css:dev & npm run webpack:dev",
    "prod": "npm run css:prod && npm run webpack:prod",
    "prod:static": "node src/scripts/generateStatic.js",
    "webpack:dev": "./node_modules/.bin/webpack --watch --d --config webpack.config.js",
    "webpack:prod": "./node_modules/.bin/webpack -p --config webpack.config.js",
    "css:build": "./node_modules/.bin/node-sass src/styles/index.scss docs/dst/styles/index.css",
    "css:dev": "./node_modules/.bin/node-sass src/styles/index.scss docs/dst/styles/index.css",
    "css:prod": "./node_modules/.bin/node-sass --output-style=compressed src/styles/index.scss docs/dst/styles/index.css",
    "lint": "./node_modules/.bin/eslint src/scripts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ko31/teamcolors.git"
  },
  "author": "Ko Takagi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teamcolors/teamcolors.github.io/issues"
  },
  "homepage": "https://github.com/ko31/teamcolors",
  "dependencies": {
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "fuzzy": "^0.1.1",
    "node-sass": "^3.4.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
